{
  "name": "nodistract",
  "displayName": "Nodistract",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "zorinos",
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev",
    "dev:next": "next dev --port 1947",
    "build": "run-p build:*",
    "build:plasmo": "plasmo build",
    "build:next": "next build"
  },
  "dependencies": {
    "next": "13.0.4",
    "plasmo": "0.59.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.202",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "prettier": "2.8.0",
    "typescript": "4.9.3",
    "@plasmohq/rps": "1.8.0",
    "@plasmohq/storage": "0.13.0",
    "@plasmohq/prettier-plugin-sort-imports": "3.6.0"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs"
    ]
  }
}
